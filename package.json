{
  "name": "ytempire",
  "version": "1.0.0",
  "description": "YouTube Automation Platform",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "node backend/server.js",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:docker": "jest --config=jest.docker.config.js --runInBand --verbose",
    "test:docker:compose": "jest tests/docker/compose.test.js --runInBand",
    "test:docker:services": "jest tests/docker/services.test.js --runInBand",
    "test:docker:integration": "jest tests/integration/*.test.js --runInBand",
    "test:docker:all": "npm run test:docker",
    "docker:up": "docker-compose up -d --build",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:ps": "docker-compose ps",
    "docker:clean": "docker-compose down -v --remove-orphans",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js"
  },
  "keywords": [
    "youtube",
    "automation",
    "content-creation",
    "analytics"
  ],
  "author": "YTEmpire Team",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@types/jest": "^29.5.0",
    "axios": "^1.11.0",
    "babel-jest": "^30.0.5",
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.6.0",
    "eslint": "^8.0.0",
    "form-data": "^4.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "pg": "^8.11.0",
    "prettier": "^2.8.0",
    "redis": "^4.6.0",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
